{
    "source": {
      "include": ["index.js"],  
      "exclude": ["node_modules"],  
      "includePattern": ".+\\.js(doc|x)?$",  
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",  
      "recurse": true,  
      "template": "templates/default"  
    }
  }
  